  font-size 100px
.wrapper
  margin 1rem auto
  padding .05rem
  background rgba(73, 90, 128, .8)
  height 4.6rem
  width 4.6rem
  border-radius .06rem
  display flex
  flex-direction column
  justify-content space-around
  .row    
    display flex
    justify-content space-around
    .cell  
      background rgba(250,250,250,.3)
      height 1rem
      width 1rem
      border-radius .04rem
      text-align center
      line-height 1rem
      font-size .5rem
.movedown
  height 20px
  width 20px
  background yellow
    